<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/parents_school"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_65"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/parents_school_iv"
            android:layout_width="@dimen/dp_38"
            android:layout_height="@dimen/dp_38"
             />

        <com.tiancaijiazu.app.utils.MediumBoldTextViewStandard
            android:id="@+id/parents_school_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/college_one"
            android:textColor="@color/colorTextTitleHome"
            android:textSize="@dimen/sp_14" />
    </LinearLayout>
</LinearLayout>