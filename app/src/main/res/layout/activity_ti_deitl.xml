<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activitys.income.TiDeitlActivity">

    <RelativeLayout
        android:id="@+id/relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_64"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@color/colorWhite">

        <ImageView
            android:id="@+id/iv_finis"
            android:layout_width="@dimen/dp_40"
            android:layout_height="@dimen/dp_40"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_2"
            android:layout_marginLeft="@dimen/dp_5"
            android:src="@mipmap/rec_back" />

        <com.tiancaijiazu.app.utils.MediumBoldTextViewStandard
            android:id="@+id/a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="@dimen/dp_11"
            android:text="提现"
            android:textColor="@color/colorTextTitleHome"
            android:textSize="@dimen/sp_17" />
    </RelativeLayout>

    <ImageView
        android:layout_marginTop="@dimen/dp_90"
        android:layout_marginLeft="@dimen/dp_110"
        android:layout_width="@dimen/dp_15"
        android:layout_height="@dimen/dp_15"
        android:background="@drawable/shape_corne_tixian"
        />
    <TextView
        android:layout_marginTop="@dimen/dp_91"
        android:layout_marginLeft="@dimen/dp_149"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="发起提现申请"
        android:textColor="#ff999999"
        android:textSize="@dimen/sp_15"
        />
    <ImageView
        android:layout_width="@dimen/dp_2"
        android:layout_height="@dimen/dp_115"
        android:src="@mipmap/xingz"
        android:layout_marginTop="@dimen/dp_98"
        android:layout_marginLeft="@dimen/dp_117"
        />
    <ImageView
        android:layout_marginTop="@dimen/dp_192"
        android:layout_marginLeft="@dimen/dp_98"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:src="@mipmap/chuli"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="平台处理中"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_17"
        android:layout_marginTop="@dimen/dp_195"
        android:layout_marginLeft="@dimen/dp_149"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="预计7天内到账"
        android:textColor="#ff999999"
        android:textSize="15sp"
        android:layout_marginTop="@dimen/dp_216"
        android:layout_marginLeft="@dimen/dp_149"
        />
    <ImageView
        android:layout_marginTop="@dimen/dp_208"
        android:layout_marginLeft="@dimen/dp_117"
        android:layout_width="@dimen/dp_2"
        android:layout_height="@dimen/dp_115"
        android:src="@mipmap/xing4"
        />
    <ImageView
        android:id="@+id/yuans"
        android:layout_marginTop="@dimen/dp_319"
        android:layout_marginLeft="@dimen/dp_110"
        android:layout_width="@dimen/dp_15"
        android:layout_height="@dimen/dp_15"
        android:background="@drawable/shape_corne_tixians"
        />
    <TextView
        android:layout_marginTop="@dimen/dp_319"
        android:layout_marginLeft="@dimen/dp_149"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="转账成功"
        android:textColor="#ff999999"
        android:textSize="@dimen/sp_17"
        />

    <View
        android:layout_marginTop="@dimen/dp_375"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        android:background="#F1F1F1"/>

    <RelativeLayout
        android:layout_marginTop="@dimen/dp_72"
        android:layout_below="@+id/yuans"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <TextView
        android:layout_marginLeft="@dimen/dp_17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提现金额"
        android:textColor="#ff999999"
        android:textSize="@dimen/sp_14"
        />

        <TextView
            android:id="@+id/qian_shu"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="￥2000.00"
            android:layout_marginRight="@dimen/dp_16"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            />

        <TextView
            android:layout_marginLeft="@dimen/dp_17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="到账银行卡"
            android:layout_marginTop="@dimen/dp_33"
            android:textColor="#ff999999"
            android:textSize="@dimen/sp_14"
            />

        <TextView
            android:id="@+id/my_card"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="工商银行 尾号9062"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_marginTop="@dimen/dp_33"
            android:textColor="#ff333333"
            android:textSize="@dimen/sp_14"
            />

        <ImageView
            android:id="@+id/finish"
            android:layout_width="@dimen/dp_180"
            android:layout_height="@dimen/dp_40"
            android:layout_centerHorizontal="true"
            android:src="@mipmap/finish"
            android:layout_marginTop="@dimen/dp_89"
            />
    </RelativeLayout>
</RelativeLayout>