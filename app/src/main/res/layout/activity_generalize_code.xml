<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    tools:context=".activitys.activitypage.myactivity.GeneralizeCodeActivity">
    <RelativeLayout
        android:id="@+id/rela"
        android:layout_width="match_parent"
        android:background="@color/colorWhite"
        android:layout_height="@dimen/dp_64">
        <ImageView
            android:id="@+id/iv_finis"
            android:layout_width="@dimen/dp_40"
            android:layout_height="@dimen/dp_40"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_2"
            android:layout_marginLeft="@dimen/dp_5"
            android:src="@mipmap/rec_back"/>
        <com.tiancaijiazu.app.utils.MediumBoldTextViewTitle
            android:id="@+id/a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_17"
            android:textColor="@color/colorTextTitleHome"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_11"
            android:text="我的推荐码"/>

    </RelativeLayout>
    <View
        android:layout_below="@+id/rela"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        android:background="#F1F1F1"/>
    <TextView
        android:id="@+id/generalize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_150"
        android:text="654 892"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_30"
        />
    <TextView
        android:id="@+id/fuzhi"
        android:background="@drawable/generalize"
        android:layout_below="@id/generalize"
        android:layout_centerHorizontal="true"
        android:layout_width="@dimen/dp_110"
        android:layout_height="@dimen/dp_30"
        android:gravity="center"
        android:text="复制推荐码"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_28"
        />
    <TextView
        android:id="@+id/fenxiang"
        android:background="@drawable/generalize"
        android:layout_below="@id/fuzhi"
        android:layout_centerHorizontal="true"
        android:layout_width="@dimen/dp_110"
        android:layout_height="@dimen/dp_30"
        android:gravity="center"
        android:text="分享推荐码"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_15"
        />
</RelativeLayout>